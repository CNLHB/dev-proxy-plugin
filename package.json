{
	"name": "dev-proxy-plugin",
	"version": "1.0.0",
	"description": "A Vite plugin for development environment proxy that automatically proxies remote server requests and handles HTML responses",
	"files": [
		"dist"
	],
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "npx unbuild",
		"prepack": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CNLHB/dev-proxy-plugin.git"
	},
	"keywords": [
		"vite",
		"vite-plugin",
		"proxy",
		"dev-proxy",
		"development",
		"html-proxy",
		"cookie",
		"redirect"
	],
	"author": "aiwa",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@types/node": "^20.11.5",
		"typescript": "^5.3.3",
		"unbuild": "^2.0.0",
		"vite": "^5.0.12"
	}
}